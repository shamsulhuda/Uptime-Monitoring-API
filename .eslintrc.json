{
  "extends": ["prettier", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "env": {
    "commonjs": true,
    "node": true
  },
  "rules": {
    "no-underscore-dangle": "off",
    "no-trailing-spaces": "warn",
    "no-console": 0,
    "indent": 0,
    "linebreak-style": 0,
    "prettier/prettier": 0
  },
  "plugins": ["prettier"]
}

